{

  "manifest_version": 2,
  "name": "feedbin-blocker",
  "description": "Block access to Feedbin unless there is more than a set amount of unread entries.",
  "version": "1.0",

  "icons": {
    "48": "icons/block.svg",
    "96": "icons/block.svg"
  },

  "browser_specific_settings": {
    "gecko": {
      "strict_min_version": "56.0a1"
    }
  },

  "background": {
    "scripts": [
      "background/request-handler.js"
    ]
  },

  "options_ui": {
    "page": "options/options.html",
    "browser_style": true
  },

  "permissions": ["webRequest", "webRequestBlocking", "storage", "notifications", "<all_urls>"]

}
